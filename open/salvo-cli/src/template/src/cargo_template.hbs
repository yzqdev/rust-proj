[package]
name = "{{project_name}}"
version = "0.1.0"
edition = "2021"

[dependencies]
{{#each dependencies}}
{{#if this.version}}
{{@key}} = { version = "{{this.version}}", features = [{{#each this.features}}"{{this}}"{{#unless @last}}, {{/unless}}{{/each}}] }
{{else}}
{{@key}} = "{{this}}"
{{/if}}
{{/each}}
{{#if is_web_site}}
askama = "0.12.0"
{{/if}}